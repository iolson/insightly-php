{
  "name": "iolson/insightly-php",
  "description": "A comprehensive Insightly API package.",
  "keywords": [
    "php",
    "insightly",
    "crm",
    "api client"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ian Olson",
      "email": "me@ianolson.io"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.8",
    "phpspec/phpspec": "~2.1",
    "scrutinizer/ocular": "~1.1",
    "fzaninotto/faker": "^1.5",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "IanOlson\\Insightly\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IanOlson\\Insightly\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
